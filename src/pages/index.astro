import Hero from "../components/Hero.astro";
import Avisos from "../components/Avisos.astro";
import Boletines from "../components/boletines.astro";
---

<!doctype html>
<html lang="en">
	<head>
		<BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
	</head>
	<body>
		<Hero />
		<Header  logoUrl="/pclogo.svg"/>
        <Avisos />
		<Boletines />
		<Footer />
	</body>
</html>
